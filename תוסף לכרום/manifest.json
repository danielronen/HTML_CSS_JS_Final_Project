{
    "manifest_version": 2,

    "name": "Daniel's extension",
    "description": "Change all images in the tab!",
    "version": "1.0",

    "icons": {
        "48": "icon.png"
    },

    "permissions": ["tabs", "<all_urls>"],

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "index.html"
    }
}